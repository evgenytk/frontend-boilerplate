{
	"extends": "airbnb",
  "parser": "babel-eslint",
	"globals": {
    	"window": true,
    	"document": true
  	},
  	"rules": {
      "indent": ["error", "tab"],
      "no-tabs": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-indent": ["error", "tab"],
      "react/self-closing-comp": 0,
  	  "dot-notation": 0,
  	  "react/jsx-filename-extension": 0,
  	  "import/extensions": 0
  	}
}